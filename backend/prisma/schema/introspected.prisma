view asset_price_5m {
  open_time   DateTime @db.Timestamp(6)
  assetInfoId String
  openPrice   Float?
  closePrice  Float?
  highPrice   Float?
  lowPrice    Float?
  volume      Float?

  @@unique([assetInfoId, open_time])
}

